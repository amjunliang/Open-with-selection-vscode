{
  "name": "open-with-selection",
  "displayName": "Open with Selection",
  "description": "Open files in VS Code with precise text selection from external applications via URI protocol",
  "version": "0.1.1",
  "publisher": "xmkj",
  "author": {
    "name": "xmkj"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amjunliang/open-with-selection-vscode"
  },
  "keywords": [
    "uri",
    "protocol",
    "external",
    "selection",
    "jump",
    "navigation",
    "integration",
    "xcode"
  ],
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onUri"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "open-with-selection.selectRange",
        "title": "Open with Selection: Select Range"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^3.6.2",
    "esbuild": "^0.19.12",
    "typescript": "^5.0.0"
  }
}
